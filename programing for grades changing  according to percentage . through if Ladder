#include <stdio.h>

int main() {
    int per;
    printf ("Enter a percentage: ");
    scanf ("%d",&per);
    if (per >= 90)
    printf("Grade is A+");
    else if (per >= 80)
    printf("Grade is A");
    else if (per >= 60)
    printf("Grade is B");
    else if (per > 50)
    printf("Grade is  c");
    else
    printf("Grade is D");
    

    return 0;
}
