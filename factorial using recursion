#include <stdio.h>

int fact (int n) {
    if(n == 1)
        return 1;
      return n * fact(n - 1);  
    
}

int main() {
    int f;
    printf("Enter the number: ");
    scanf("%d", &f);
    int result = fact(f);  // Correct function call
    printf("Factorial of %d is: %d\n", f, result);  // Output the result
    return 0;
}
