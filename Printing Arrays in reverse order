#include <stdio.h>

int main() {
    int n, number;

    printf("How many numbers do you want to enter: ");
    scanf("%d", &n);

    int data[n];

    for (int i = 0; i < n; i++) {
        printf("Enter a number: ");
        scanf("%d", &number);
        data[i] = number;
    }

    printf("\nResult is\n");
    for (int i = n - 1; i >= 0; i--) {
        printf("%d ", data[i]);
    }

    return 0;
}
