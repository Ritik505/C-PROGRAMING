#include <stdio.h>

int main() {
    int n1,n2,i,result=0;
    
    printf("\nEnter first number: ");
    scanf("%d",&n1);
    printf("\nEnter second number : ");
    scanf("%d",&n2);
    
    for(i=1;i<=n2;i++)
    {
        result += n1;
    }
    printf("\nProduct of %d and %d is %d",n1,n2,result);
    

    return 0;
}
