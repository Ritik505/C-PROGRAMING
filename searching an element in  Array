#include <stdio.h>

int main() {
    int a[10] ={10,20,34,233,8,45,12,34,9,56};
    int num,i,found=0;
    printf("\n Enter the number to be searched:");
    scanf("%d",&num);
    for (i=0;i<10;i++)
    {
        if (a[i] == num)
        {
            printf("\n found at position %d",i);
            found++;
        }
    }
    if (found == 0)
    printf("\nNo.not found");
    return 0;
}
