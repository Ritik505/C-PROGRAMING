#include <stdio.h>

int main() {
    int num[5];  // Array for five numbers
    int i;
    
    printf("Enter any five numbers: ");
    
    // Input loop, index starting from 0
    for(i = 0; i < 5; i++) {
        scanf("%d", &num[i]);
    }
    
    printf("\nEntered numbers are: ");
    
    // Loop to check each number
    for(i = 0; i < 5; i++) {
        if(num[i] % 2 == 0) {  // Check if number is even
            printf("\n%d is even", num[i]);
        } else {  // Otherwise, it's odd
            printf("\n%d is odd", num[i]);
        }
    }
    
    return 0;
}

    

    return 0;
}
