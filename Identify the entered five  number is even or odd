#include <stdio.h>

int main() {
    int i,num;
    printf("Enter any five numbers: ");
    
    for(i=1;i<=5;i++)
    {
        scanf("%d",&num);
        if(num%2==0)
            printf("number is even\n");
        
        else
            printf("number is odd\n");
        
    }
    

    return 0;
}
