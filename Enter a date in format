#include <stdio.h>

int main() {
    int d,m,y;
    printf("\nEnter date: ");
    scanf("%d",&d);
    if (d>32)
    {
        printf("Enter date with in 32");
    }
    printf("\nEnter month: ");
    scanf("%d",&m);
    if (m>12)
    {
        printf("Enter month with in 12");
    }
    printf("\n");
    printf("Enter a year: ");
    scanf("%d",&y);
    printf("\nEntered date is %d/%d/%d",d,m,y);
    return 0;
}
