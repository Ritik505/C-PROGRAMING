#include <stdio.h>

int main() {
    int num,dg,rev=0,a;
    printf("Enter the number: ");
    scanf("%d",&num);
    a=num;
    while(num>0){
        dg=num%10;
        rev=rev*10+dg;
        num=num/10;
    }
    if(a==rev)
    printf("number is palindrome");
    else
    printf("Number is not palindrome");
    return 0;
}
