#include <stdio.h>

int main() {
    int a,i,s,beg,end,mid;
    printf("Enter number of elements: ");
    scanf("%d",&a);
    int array[a];
    printf("Enter the element in sorted manner:\n");
    for(i=0;i<a;i++)
    scanf("%d",&array[i]);
    printf("\nEnter the number to be searched: ");
    scanf("%d",&s);
    beg=0;
    end=a-1;
    while(beg<=end)
    {
        mid=(beg+end)/2;
        if(array[mid]==s)
        {
            printf("\nFound at position %d",mid);
            break;
        }
        else if(s>array[mid])
        beg = mid+1;
        else if(s<array[mid])
        end = mid-1;
    }
    if(beg>end)
    {
        printf("\nNot found");
    }
    return 0;
}#include <stdio.h>

int main() {
    int a,i,s,beg,end,mid;
    printf("Enter number of elements: ");
    scanf("%d",&a);
    int array[a];
    printf("Enter the element in sorted manner:\n");
    for(i=0;i<a;i++)
    scanf("%d",&array[i]);
    printf("\nEnter the number to be searched: ");
    scanf("%d",&s);
    beg=0;
    end=a-1;
    while(beg<=end)
    {
        mid=(beg+end)/2;
        if(array[mid]==s)
        {
            printf("\nFound at position %d",mid);
            break;
        }
        else if(s>array[mid])
        beg = mid+1;
        else if(s<array[mid])
        end = mid-1;
    }
    if(beg>end)
    {
        printf("\nNot found");
    }
    return 0;
}
